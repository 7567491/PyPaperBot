          }
        },
        init() {
          if (!window.modalStack) {
            window.modalStack = [];
            window.addEventListener('keydown', (event) => {
              if (event.key === 'Escape' && window.modalStack.length > 0) {
                window.modalStack[window.modalStack.length - 1]();
              }
            });
          }

          this.$watch('shareModal', (open) => {
            if (open) {
              window.modalStack.push(this.close.bind(this));
            } else {
              window.modalStack.pop();
            }
          });
        },
      }"
