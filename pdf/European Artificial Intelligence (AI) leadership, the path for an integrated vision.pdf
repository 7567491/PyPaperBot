  <script>
    document.addEventListener("alpine:init", () => {
      Alpine.data("itemsContainer", (object_id, content_type) => ({
        data: [],
        loading: true,
        newListName: "",
        newList: false,
        isBulkUpdate: object_id === "search" && content_type === "search",
        async loadLists() {
          const url = "/api/lists/";

          try {
            const response = await fetch(
              url +
                "?" +
                new URLSearchParams({
                  object_id,
                  content_type,
                  item_ids: this.isBulkUpdate,
                }),
              {
                method: "GET",
                credentials: "include",
                headers: {
                  "Content-Type": "application/json",
                },
              },
            );

            if (!response.ok) {
              throw new Error(response.statusText);
            }
            this.data = await response.json();
            console.log(this.data);
          } catch (err) {
            console.error(err);
            toast({ message: "Failed to fetch lists", type: "warning" });
          } finally {
            this.loading = false;
          }
        },
        async createList() {
          const url = "/api/lists/create/";

          try {
            const response = await fetch(url, {
              method: "POST",
              credentials: "include",
              headers: {
                "Content-Type": "application/json",
                "X-CSRFToken": document.querySelector("[name=csrfmiddlewaretoken]").value,
              },
              body: JSON.stringify({ title: this.newListName }),
            });

            if (!response.ok) {
              throw new Error(response.statusText);
            }

            const data = await response.json();
            this.newListName = "";
            this.newList = false;
            await this.loadLists();
          } catch (err) {
            console.error(err);
            toast({ message: "Error creating list", type: "warning" });
          }
        },
        async updateItem(id, action, onSuccess) {
          if (this.isBulkUpdate) return this.updateItems(id, action, onSuccess);

          const url = "/api/lists/add_item/";
          const args = {
            added_list_ids: action === "add" ? [id.toString()] : [],
            deleted_list_ids: action === "remove" ? [id.toString()] : [],
            object_id: object_id,
            content_type: content_type,
          };

          try {
            const response = await fetch(url, {
              method: "POST",
              credentials: "include",
              headers: {
                "Content-Type": "application/json",
                "X-CSRFToken": document.querySelector("[name=csrfmiddlewaretoken]").value,
              },
              body: JSON.stringify(args),
            });

            if (!response.ok) {
              throw new Error(response.statusText);
            }

            onSuccess();

            const listIndex = this.data.findIndex((list) => list.id === id);
            const list = this.data[listIndex];
            if (action === "add") {
              list.checked = true;
              list.size += 1;
            }
            if (action === "remove") {
              list.checked = false;
              list.size -= 1;
            }
          } catch (err) {
            console.error(err);
            toast({ message: "Error updating list", type: "warning" });
          }
        },

        async updateItems(id, action, onSuccess) {
          const url = "/api/lists/add_item_bulk/";
          const args = {
            added_list_ids: action === "add" ? [id.toString()] : [],
            deleted_list_ids: action === "remove" ? [id.toString()] : [],
            objects: Array.from(document.querySelectorAll(".search-item")).map((b) => ({
              object_id: b.dataset.id,
              content_type: b.dataset.type,
            })),
          };

          try {
            const response = await fetch(url, {
              method: "POST",
              credentials: "include",
              headers: {
                "Content-Type": "application/json",
                "X-CSRFToken": document.querySelector("[name=csrfmiddlewaretoken]").value,
              },
              body: JSON.stringify(args),
            });

            if (!response.ok) {
              throw new Error(response.statusText);
            }

            onSuccess();

            const listIndex = this.data.findIndex((list) => list.id === id);
            const list = this.data[listIndex];
            if (action === "add") {
              const newItems = args.objects.filter(
                (o) => !list.item_ids.includes(Number(o.object_id)),
              );
              list.checked = true;
              list.size += newItems.length;
              list.item_ids = [...list.item_ids, ...newItems.map((o) => Number(o.object_id))];
            }
            if (action === "remove") {
              const removedItems = args.objects.filter((o) =>
                list.item_ids.includes(Number(o.object_id)),
              );
              list.checked = false;
              list.size -= removedItems.length;
              list.item_ids = list.item_ids.filter(
                (id) => !removedItems.some((o) => Number(o.object_id) === id),
              );
            }
          } catch (err) {
            console.error(err);
            toast({ message: "Error updating list", type: "warning" });
          }
        },
      }));
    });
  </script>

  
  <script>
    document.addEventListener("alpine:init", () => {
      Alpine.data("scrollIndicator", (open) => ({
        isScrollable: false,
        isAtTop: false,
        isAtBottom: false,

        init() {
          this.$watch(open, (value) => {
            if (value) {
              setTimeout(() => {
                this.$nextTick(() => {
                  this.$refs.scrollIndicatorContainer.scrollTo(0, 0);
                  this.checkScrollable();
                  this.checkScroll();
                });
              }, 100);
            }
          });
        },

        checkScrollable() {
          this.isScrollable =
            this.$refs.scrollIndicatorContainer.scrollHeight > this.$refs.scrollIndicatorContainer.clientHeight;
        },

        checkScroll() {
          const container = this.$refs.scrollIndicatorContainer;
          this.isAtTop = container.scrollTop === 0;
          this.isAtBottom =
            container.scrollTop + container.clientHeight >= container.scrollHeight - 1; // -1 to account for rounding errors
        },
      }));
    });
  </script>

  
  <script>
    document.addEventListener("alpine:init", () => {
      Alpine.data("dropdown", function () {
        return {
          dropdownOpen: false,
          focusableElements: [],
          currentFocusIndex: -1,
          toggle() {
            if (this.dropdownOpen) {
              return this.close();
            }
            this.$refs.button.focus();
            this.dropdownOpen = true;
            this.$nextTick(() => {
              this.focusableElements = this.$refs.panel.querySelectorAll(
                'a, button, input, select, textarea, [tabindex]:not([tabindex="-1"])',
              );
              this.currentFocusIndex = -1;
            });
          },
          close(focusAfter) {
            if (!this.dropdownOpen) return;
            this.dropdownOpen = false;
            focusAfter && focusAfter.focus();
          },
          handleKeyDown(event) {
            if (!this.dropdownOpen) return;

            switch (event.key) {
              case "ArrowDown":
                event.preventDefault();
                this.currentFocusIndex =
                  (this.currentFocusIndex + 1) % this.focusableElements.length;
                this.focusableElements[this.currentFocusIndex].focus();
                break;
              case "ArrowUp":
                event.preventDefault();
                this.currentFocusIndex =
                  (this.currentFocusIndex - 1 + this.focusableElements.length) %
                  this.focusableElements.length;
                this.focusableElements[this.currentFocusIndex].focus();
                break;
              case "Tab":
                event.preventDefault();
                this.close();
                break;
            }
          },
        };
      });
    });
  </script>

  
  <script>
    function scrollContainer() {
      return {
        canScrollLeft: false,
        canScrollRight: true,
        setButtonVisibility() {
          const container = this.$refs.scrollContainer;
          this.canScrollLeft = container.scrollLeft > 0;
          this.canScrollRight =
            container.scrollLeft < container.scrollWidth - container.clientWidth - 0.5;
        },
        scrollLeft() {
          const container = this.$refs.scrollContainer;
          const newPos = container.scrollLeft - container.clientWidth / 2;
          container.scrollTo({ left: newPos, behavior: "smooth" });
        },
        scrollRight() {
          const container = this.$refs.scrollContainer;
          const newPos = container.scrollLeft + container.clientWidth / 2;
          container.scrollTo({ left: newPos, behavior: "smooth" });
        },
      };
    }
  </script>

  

