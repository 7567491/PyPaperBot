  <script>
    function topicContainer(following, authenticated) {
      return {
        following,
        authenticated,
        follow(id) {
          fetch("/api/follow/", {
            method: "POST",
            credentials: "include",
            headers: {
              "Content-Type": "application/json",
              "X-CSRFToken": document.querySelector("[name=csrfmiddlewaretoken]").value,
            },
            body: JSON.stringify({
              content_type: "topic",
              object_id: id,
            }),
          })
            .then((response) => {
              if (!response.ok) {
                throw new Error("Network response was not ok");
              }
              return response.json();
            })
            .catch((error) => {
              console.error("Error:", error);
            })
            .finally(() => {
              this.following = true;
            });
        },
      };
    }
  </script>
